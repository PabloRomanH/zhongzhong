{
   "background": {
      "scripts": [ "dict.js", "main.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "images/zhongzhong48.png",
      "default_title": "Zhongzhong Chinese Popup Dictionary"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/jquery-1.7.1.min.js", "content.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Chinese popup translator. Great tool for learning Chinese. Frequently updated.",
   "icons": {
      "128": "images/zhongzhong.png",
      "16": "images/zhongzhong16.png",
      "48": "images/zhongzhong48.png"
   },
   "manifest_version": 2,
   "name": "Zhongzhong: An improved Chinese Dictionary",
   "options_page": "options.html",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.5",
   "web_accessible_resources": [ "css/*", "js/*", "images/*" ]
}
